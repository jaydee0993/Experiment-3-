#include <iostream>
using namespace std;

int main ()
{
	int size;
	cout << "Enter array size: ";
	cin >> size;
	char arr[size];
	cout << "Enter character elements: ";
	for (int i=0; i<size; i++)
	{
		cin >> arr[i];
	}
	cout << "Reverse array: ";
	for (int i=size; i>=0; i--)
	{
		cout << arr[i] << " ";
	}
	
	
	return 0;
}
